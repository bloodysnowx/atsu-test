Fend

Procedure ChargeAt(x)
    Print GetTimeString() + "ChargeAt " + x
    Go補給()
    Select補給艦隊(x)
    Select補給All()
    Clickまとめて補給()
    GoBackHome()
    EnseiKakuninIfNeed()
Fend

Procedure EnseiKakuninIfNeed
    color = PEEKCOLOR(3475 - 2871, 221 + 20)
    While color = $ACAC2D
        EnseiKakunin()
        color = PEEKCOLOR(3475 - 2871, 221 + 20)
    Wend
Fend

// 26s
Procedure EnseiKakunin
