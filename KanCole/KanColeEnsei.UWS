Procedure Main2()
    ThreeTwoOneCount = 0
    ChokyoriCount = 0
    KaijoGoeiCount = 0
    KantaiKessenCount = 0
    WHILE True
        RefreshIfNeed()
	// ThreeTwoOneCount = GoThreeTwoOneMain(ThreeTwoOneCount)
        For i = 0 to 3
	    ResetConfig()
	    ResetToHome()
	    RefreshIfNeed()
	    ChokyoriCount = GoEnseiIfNeedAt(1, ChokyoriCount)
	    KaijoGoeiCount = GoEnseiIfNeedAt(2, KaijoGoeiCount)
	    KantaiKessenCount = GoEnseiIfNeedAt(3, KantaiKessenCount)
	    SleepAndPrint(3 * 60 + RANDOM(30))
	Next
    WEND
Fend

Procedure ChargeAt(x)
    Print GetTimeString() + "ChargeAt " + x
    Go補給()
    Select補給艦隊(x)
    Select補給All()
    Clickまとめて補給()
    GoBackHome()
    EnseiKakuninIfNeed()
Fend

Procedure EnseiKakuninIfNeed
    color = PEEKCOLOR(3475, 221)
    While color = $ACAC2D
        EnseiKakunin()
        color = PEEKCOLOR(3475, 221)
    Wend
Fend

